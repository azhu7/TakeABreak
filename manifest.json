{
  "manifest_version": 2,
  "name": "Take a Break!",
  "version": "0.3",
    "background": {
    "scripts": ["background.js", "alarm.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "content.js" ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "omnibox": { "keyword" : "alarm" },
  "permissions": [ "alarms" ]
}